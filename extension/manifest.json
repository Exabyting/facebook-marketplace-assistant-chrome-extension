{
    "name": "Marketplace Assistant",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "permissions": ["activeTab", "tabs", "scripting"],
    "host_permissions": ["https://www.facebook.com/*"],
    "background": {
        "service_worker": "background.js"
    }
}
